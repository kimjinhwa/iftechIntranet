{
  "name": "iftintranet",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
	  "devmon": "nodemon --watch \"*.js,./public/**/*.js,./routes/*.js,./views/*.pug,./stylesheets/*.css, \" --exec \"npm start\"",
	  "dev": "browser-sync start --proxy http://localhost:3000 --files './stylesheets/*.css,./public/**/*.*',./views/*.pug,routes/*.js/"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  }
}
